	option dns '8.8.8.8'
	option dns2 '8.8.4.4'
	option device_ip 'checked'
	option startip '192.168.2.200'
	option endip '192.168.2.240'
	option router '192.168.2.3'

config secn 'http'
	option limitip '0'
	option enssl '0'
	option pw_preset '1'
	option auth 'checked'

config secn 'wan'
	option wanip '10.0.0.20'
	option secwanip '10.0.1.20'
	option wangateway '10.0.0.1'
	option wanmask '255.255.255.0'

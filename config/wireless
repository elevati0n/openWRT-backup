
config wifi-device 'radio0'
	option type 'mac80211'
	option phy 'phy0'
	option htmode 'HT20'
	option channel '1'
	option txpower '17'
	option chanbw '20'
	option hwmode '11ng'
	option disabled '0'

config wifi-iface 'ap_0'
	option device 'radio0'
	option ifname 'wlan0'
	option network 'lan'
	option mode 'ap'
	option maxassoc '20'
	option encryption 'psk2'
	option key 'potato-potato'
	option hidden '0'
	option disabled '0'
	option isolate '0'
	option ssid 'BatNode1'

config wifi-iface 'ah_0'
	option device 'radio0'
	option ifname 'wlan0-1'
	option mode 'adhoc'
	option network 'mesh_0'
	option key 'mesh-mesh'
	option disabled '0'
	option hidden '0'
	option encryption 'off'
	option bssid '02:12:34:56:78:9A'
	option ssid 'batmesh'

config wifi-iface 'sta_0'
	option device 'radio0'
	option ifname 'wlan0-2'
	option mode 'sta'
	option network 'wan'
	option ssid 'host-ssid'
	option disabled '1'
	option key 'host-password'
	option encryption 'mixed-psk'

